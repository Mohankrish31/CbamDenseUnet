{
  "name": "default",
  "mode": "train",
  "device": {
    "name": "cuda"
  },
  "model": {
    "which_model": {
      "name": "CBAM_DenseUNet",
      "args": {
        "in_channels": 3,
        "out_channels": 3,
        "features": 64
      }
    }
  },
  "dataset": {
    "args": {
      "low_dir": "/content/CVC-ColonDB-Split/train/low",
      "high_dir": "/content/CVC-ColonDB-Split/train/high",
      "transform": true,
      "paired": true
    }
  },
  "dataloader": {
    "args": {
      "batch_size": 8,
      "shuffle": true,
      "num_workers": 2,
      "pin_memory": true
    }
  },
  "output_dir": "outputs/train_logs",
  "model_path": "saved_models/cbam_train_model.pth",
  "training_info": {
    "epoch": 100,
    "learning_rate": 0.0001
  }
}
