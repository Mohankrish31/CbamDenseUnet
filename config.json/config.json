{
  "name": "cbam_denseunet_config",
  "model": {
    "which_model": {
      "name": [
        "models.cbam_denseunet",
        "CBAM_DenseUNet"
      ],
      "args": {}
    }
  },
  "train": {
    "device": "cuda",
    "n_epoch": 80,
    "lr": 1e-4,
    "dataset": {
      "name": [
        "dataset.cvccolondb",
        "cvccolondb"
      ],
      "args": {
        "low_light_root": "/content/cvc-colondb/train/low",
        "normal_light_root": "/content/cvc-colondb/train/high",
        "image_size": [224, 224]
      }
    },
    "dataloader": {
      "args": {
        "batch_size": 8,
        "shuffle": true,
        "num_workers": 2
      }
    },
    "model_path": "saved_models/",
    "model_name": "CBAM_DenseUNet.pt"
  },
  "validation": {
    "device": "cuda",
    "dataset": {
      "name": [
        "dataset.cvccolondb",
        "cvccolondb"
      ],
      "args": {
        "low_light_root": "/content/cvc-colondb/val/low",
        "normal_light_root": "/content/cvc-colondb/val/high",
        "image_size": [224, 224]
      }
    },
    "dataloader": {
      "args": {
        "batch_size": 8,
        "shuffle": false,
        "num_workers": 2
      }
    }
  },
  "test": {
    "device": "cuda",
    "dataset": {
      "name": [
        "dataset.cvccolondb",
        "cvccolondb"
      ],
      "args": {
        "low_light_root": "/content/cvc-colondb/test/low",
        "normal_light_root": "/content/cvc-colondb/test/high",
        "image_size": [224, 224]
      }
    },
    "dataloader": {
      "args": {
        "batch_size": 1,
        "shuffle": false,
        "num_workers": 2
      }
    },
    "model_path": "saved_models/",
    "model_name": "CBAM_DenseUNet.pt",
    "output_images_path": "output_images/"
  }
}
