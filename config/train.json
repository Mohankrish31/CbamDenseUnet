{
  "model": {
    "which_model": {
       "name": "CBAM_DenseUNet",
      "args": {
        "in_channels": 3,
        "out_channels": 3,
        "features": 64
      }
    }
  },
  "dataset": {
    "args": {
      "low_dir": "/content/dataset/train/low",
      "high_dir": "/content/dataset/train/high",
      "transform": true
    }
  },
  "dataloader": {
    "args": {
      "batch_size": 4,
      "shuffle": true,
      "num_workers": 2,
      "pin_memory": true
    }
  },
  "lr": 0.0002,
  "n_epoch": 50,
  "model_path": "saved_models",
  "model_name": "cbam_train_model.pth",
  "save_to_drive": true
}

